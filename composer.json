{
  "name": "mrsatik/settings",
  "description": "Settings manager",
  "minimum-stability": "stable",
  "keywords": [
    "settings",
    "env",
    "file"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Perchenok Alexey",
      "email": "silenthill.sat@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "mrsatik/servers": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "mrsatik\\Settings\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mrsatik\\SettingsTest\\": "tests/"
    }
  },
  "scripts": {
